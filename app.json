{
  "expo": {
    "name": "focus-time",
    "slug": "focus-time",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.webgrammer.focustime"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This app will help you to keep always stay focused.",
    "githubUrl": "https://github.com/FahimMontasir/focus-time"
  }
}
